// Count Equal and Divisible Pairs in an Array
/**
 * @param {number[]} nums
 * @param {number} k
 * @return {number}
 */
var countPairs = function (nums, k) {
    let output = 0;
    for (let i = 0; i < nums.length - 1; i++) {
        for (j = i + 1; j < nums.length; j++) {
            if(nums[i]===nums[j] && (i*j)%k ===0){
                output++;
            }
        }
    }
    return output
};
